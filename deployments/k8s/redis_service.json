{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "redis-sentinel"
  },
  "spec": {
    "type": "LoadBalancer",
    "selector": {
      "app": "mm",
      "tier": "storage"
    },
    "ports": [
      {
        "protocol": "TCP",
        "port": 6379,
        "targetPort": "redis"
      }
    ]
  }
}
