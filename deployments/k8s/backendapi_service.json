{
  "kind": "Service",
  "apiVersion": "v1",
  "metadata": {
    "name": "om-backendapi"
  },
  "spec": {
    "type": "LoadBalancer",
    "selector": {
      "app": "openmatch",
      "component": "backend"
    },
    "ports": [
      {
        "protocol": "TCP",
        "port": 50505,
        "targetPort": "grpc"
      }
    ]
  }
}
